projection;
use draft;
//strict ( 2 );

define behavior for ZC_BTC_Travel alias Travel
//use etag
{
  use create;
  use update;
  use delete;

  use association _Booking { create; with draft; }

  use action acceptTravel;
  use action rejectTravel;
}

define behavior for ZC_BTC_Booking alias Booking
//use etag
{
  use update;
  use delete;

  use association _Travel { with draft; }
}